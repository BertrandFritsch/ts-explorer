{
  "name": "@bertrand.fritsch/ts-explorer",
  "version": "1.0.3",
  "description": "Explore TypeScript code",
  "module": "dist/main.mjs",
  "scripts": {
    "tsc:watch": "tsc -w",
    "tsc:check": "tsc --noEmit",
    "build": "node esbuild.config.mjs",
    "release": "semantic-release",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "explorer",
    "dependency",
    "graph",
    "file",
    "list"
  ],
  "author": "Bertrand Fritsch",
  "license": "MIT",
  "homepage": "https://github.com/BertrandFritsch/ts-explorer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BertrandFritsch/ts-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/BertrandFritsch/ts-explorer/issues"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "fast-glob": "^3.3.2",
    "ts-morph": "^22.0.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "esbuild": "^0.21.5",
    "esbuild-plugin-copy": "^2.1.1",
    "prettier": "^3.3.2",
    "semantic-release": "^24.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "ts-explorer": "./dist/bin/cli.mjs"
  }
}
