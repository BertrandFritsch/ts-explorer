{
  "name": "@bertrand.fritsch/ts-explorer",
  "version": "2.0.3",
  "description": "Explore TypeScript code",
  "module": "dist/main.mjs",
  "scripts": {
    "tsc:watch": "tsc -w",
    "tsc:check": "tsc --noEmit",
    "build": "node esbuild.config.mjs"
  },
  "keywords": [
    "typescript",
    "explorer",
    "dependency",
    "graph",
    "file",
    "list"
  ],
  "author": "Bertrand Fritsch",
  "license": "MIT",
  "homepage": "https://github.com/BertrandFritsch/ts-explorer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BertrandFritsch/ts-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/BertrandFritsch/ts-explorer/issues"
  },
  "dependencies": {
    "@bertrand.fritsch/ts-explorer-lib": "workspace:*",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.2",
    "ts-morph": "^23.0.0",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "esbuild": "^0.23.1",
    "esbuild-plugin-copy": "^2.1.1",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "ts-explorer": "dist/bin/cli.mjs"
  }
}
