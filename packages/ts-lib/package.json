{
  "name": "@bertrand.fritsch/ts-lib",
  "version": "2.3.0",
  "description": "Shared lib for ts-explorer plugins",
  "module": "dist/main.mjs",
  "type": "module",
  "typings": "dist/main.d.mjs",
  "scripts": {
    "build": "tsc",
    "tsc:check": "tsc --noEmit",
    "tsc:watch": "tsc --watch"
  },
  "peerDependencies": {
    "fast-glob": "^3.3.2",
    "ts-morph": "^24.0.0",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "esbuild": "^0.25.0",
    "esbuild-plugin-copy": "^2.1.1",
    "prettier": "^3.5.1"
  },
  "keywords": [
    "typescript",
    "explorer",
    "dependency",
    "graph",
    "file",
    "list"
  ],
  "author": "Bertrand Fritsch",
  "license": "MIT",
  "homepage": "https://github.com/BertrandFritsch/ts-explorer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BertrandFritsch/ts-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/BertrandFritsch/ts-explorer/issues"
  },
  "engines": {
    "node": ">=22"
  },
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "types": "./dist/main.d.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
